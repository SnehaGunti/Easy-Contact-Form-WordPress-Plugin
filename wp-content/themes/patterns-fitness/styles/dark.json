{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Dark",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#212529",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#fff",
					"name": "Default",
					"slug": "default"
				},
				{
					"color": "#343A40",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#0F0F0F",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#495057",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#E0E0E0",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#DBDBDB",
					"name": "Quinary",
					"slug": "quinary"
				},
				{
					"color": "#C3C3C3",
					"name": "Quinary Contrast",
					"slug": "quinary-contrast"
				},
				{
					"color": "#1E1E1E",
					"name": "Accent",
					"slug": "accent"
				}
			],
			"duotone": [
				{
					"colors": ["#212529", "#212529"],
					"slug": "base",
					"name": "Base"
				},
				{
					"colors": ["#fff", "#fff"],
					"slug": "white",
					"name": "White"
				},
				{
					"colors": ["#343A40", "#343A40"],
					"slug": "primary",
					"name": "Primary"
				},
				{
					"colors": ["#0F0F0F", "#0F0F0F"],
					"slug": "secondary",
					"name": "Secondary"
				},
				{
					"colors": ["#495057", "#495057"],
					"slug": "tertiary",
					"name": "Tertiary"
				},
				{
					"colors": ["#E0E0E0", "#E0E0E0"],
					"slug": "quaternary",
					"name": "Quaternary"
				},
				{
					"colors": ["#DBDBDB", "#DBDBDB"],
					"slug": "quinary",
					"name": "Quinary"
				},
				{
					"colors": ["#C3C3C3", "#C3C3C3"],
					"slug": "quinary-contrast",
					"name": "Quinary Contrast"
				},
				{
					"colors": ["#1E1E1E", "#1E1E1E"],
					"slug": "accent",
					"name": "Accent"
				}
			]
		}
	},
	"styles": {
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|primary",
					"text": "var:preset|color|default"
				},
				":hover": {
					"color": {
						"background": "var:preset|color|tertiary"
					}
				},
				":focus": {
					"color": {
						"background": "var:preset|color|tertiary"
					}
				},
				":active": {
					"color": {
						"background": "var:preset|color|tertiary"
					}
				},
				":visited": {
					"color": {
						"text": "var:preset|color|base"
					}
				}
			},

			"link": {
				"color": {
					"text": "var:preset|color|primary"
				},
				":active": {
					"color": {
						"text": "var:preset|color|primary"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "var(--wp--preset--color--secondary) !important"
						},
						"color": {
							"text": "var:preset|color|secondary"
						},
						"elements": {
							"link": {
								":hover": {
									"border": {
										"color": "var:preset|color|base"
									}
								}
							}
						}
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|quaternary"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|primary"
							}
						},
						":active": {
							"color": {
								"text": "var:preset|color|primary"
							}
						},
						":visited": {
							"color": {
								"text": "var:preset|color|primary"
							}
						}
					}
				}
			}
		}
	}
}
